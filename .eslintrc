{
  "extends": ["airbnb", "prettier", "prettier/react"],
  "plugins": ["prettier"],
  "rules": {
    "import/imports-first": ["error", "absolute-first"],
    "import/newline-after-import": "error",
    "no-underscore-dangle": 0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "react/prop-types": 0,
    "semi": [2, "never"]
  },
  "globals": {
    "clipboard": true,
    "document": true,
    "ipc": true
  },
  "parser": "babel-eslint"
}
